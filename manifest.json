{
  "name": "Paper Memory",
  "version": "0.3.3",
  "manifest_version": 2,
  "description": "Automatically record papers you read from Arxiv, OpenReview & more! Organize your library with tags, links to code and quick notes.",
  "homepage_url": "https://github.com/vict0rsch/PaperMemory",
  "icons": {
    "192": "icons/favicon-192x192.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "src/shared/jquery.min.js",
      "src/shared/utils.min.js",
      "src/background/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/favicon-192x192.png",
    "default_title": "PaperMemory",
    "default_popup": "src/popup/min/popup.min.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "permissions": [
    "https://arxiv.org/*",
    "*://api.biorxiv.org/*",
    "https://export.arxiv.org/*",
    "*://*.neurips.cc/*",
    "*://*.nips.cc/*",
    "*://proceedings.mlr.press/*",
    "https://openaccess.thecvf.com/*",
    "https://openreview.net/*",
    "https://api.openreview.net/*",
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://arxiv.org/*",
        "*://*.biorxiv.org/*",
        "*://proceedings.mlr.press/*",
        "*://*.neurips.cc/*",
        "*://*.nips.cc/*",
        "*://openaccess.thecvf.com/*",
        "*://openreview.net/*"
      ],
      "run_at": "document_start",
      "js": [
        "src/shared/jquery.min.js",
        "src/shared/utils.min.js",
        "src/content_scripts/content_script.js"
      ],
      "css": [
        "src/shared/loader.css",
        "src/content_scripts/content_script.css"
      ]
    }
  ]
}